{
    "name": "bw-eventcalendar",
    "version": "1.0.0",
    "description": "Simple Event Calendar with Admin Panel",
    "main": "server/index.js",
    "scripts": {
        "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
        "server:dev": "nodemon server/index.js",
        "client:dev": "cd client && npm run dev",
        "build": "cd client && npm install && npm run build",
        "start": "node server/index.js"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.11.3"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "nodemon": "^3.0.3"
    }
}